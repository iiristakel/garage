@model WebApp.ViewModels.WorkObjectCreateEditViewModel

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<div class="form-group">
    <label asp-for="WorkObject.ClientId" class="control-label"></label>
    <select asp-for="WorkObject.ClientId" class="form-control" asp-items="Model.ClientSelectList"></select>
    <span asp-validation-for="WorkObject.ClientId" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="WorkObject.From" class="control-label"></label>
    <input asp-for="WorkObject.From" class="form-control"/>
    <span asp-validation-for="WorkObject.From" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="WorkObject.Until" class="control-label"></label>
    <input asp-for="WorkObject.Until" class="form-control"/>
    <span asp-validation-for="WorkObject.Until" class="text-danger"></span>
</div>
<div class="form-group">
    <input type="submit" value="@Resources.Views.Common.Save" class="btn btn-primary"/>
</div>