@model WebApp.ViewModels.ProductsForClientsCreateEditViewModel

@{
    ViewData["Title"] = "Edit";
}

<h1>@Resources.Views.Common.Edit</h1>

<h4>@Resources.Domain.ProductForClient.ProductForClientString</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <input type="hidden" asp-for="ProductForClient.Id" />

            <partial name="PartialCreateEdit" model="Model"/>

        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@Resources.Views.Common.BackToList</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
